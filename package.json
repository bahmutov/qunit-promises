{
  "name": "qunit-promises",
  "description": "QUnit plugin that adds assertions to check promises quickly",
  "version": "0.1.5",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/bahmutov/qunit-promises/issues"
  },
  "contributors": [],
  "dependencies": {
    "allong.es": "0.14.0",
    "lodash": "3.10.1"
  },
  "devDependencies": {
    "coveralls": "2.11.4",
    "grunt": "0.4.5",
    "grunt-bump": "0.6.0",
    "grunt-cli": "1.2.0",
    "grunt-complexity": "0.3.0",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-qunit": "0.7.0",
    "grunt-deps-ok": "0.8.0",
    "grunt-nice-package": "0.9.4",
    "grunt-node-qunit": "2.0.2",
    "grunt-npm2bower-sync": "0.9.1",
    "gt": "0.8.49",
    "jshint-stylish": "2.0.1",
    "lcov-filter": "0.0.3",
    "matchdep": "1.0.0",
    "pre-git": "3.9.1",
    "q": "2.0.3",
    "qunit": "0.9.1",
    "time-grunt": "1.2.2"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "homepage": "https://github.com/bahmutov/qunit-promises",
  "keywords": [
    "plugin",
    "promises",
    "qunit",
    "test"
  ],
  "license": "MIT",
  "main": "qunit-promises.js",
  "files": [
    "qunit-promises.js",
    "bower.json",
    "History.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/qunit-promises.git"
  },
  "scripts": {
    "coveralls": "node node_modules/lcov-filter/index.js cover/lcov.info test | ./node_modules/coveralls/bin/coveralls.js",
    "gt": "gt qunit-promises.js test/node-tests.js --output",
    "test": "grunt && npm run gt",
    "test-qunit": "qunit -d ./qunit-promises.js -c test/tests.js -t test/tests.js",
    "commit": "commit-wizard",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run test",
        "npm run gt"
      ],
      "pre-push": [
        "npm run grunt qunit",
        "npm run size"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}