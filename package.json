{
  "name": "qunit-promises",
  "description": "QUnit plugin that adds assertions to check promises quickly",
  "version": "0.1.3",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/bahmutov/qunit-promises/issues"
  },
  "dependencies": {
    "allong.es": "~0.14.0",
    "lodash": "~2.4.1"
  },
  "devDependencies": {
    "coveralls": "2.11.1",
    "grunt": "0.4.5",
    "grunt-bump": "0.0.15",
    "grunt-complexity": "0.2.0",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-contrib-qunit": "0.5.2",
    "grunt-deps-ok": "0.5.1",
    "grunt-nice-package": "~0.2.2",
    "grunt-node-qunit": "~2.0.2",
    "grunt-npm2bower-sync": "0.4.0",
    "gt": "0.8.41",
    "jshint-stylish": "0.4.0",
    "lcov-filter": "0.0.1",
    "matchdep": "~0.3.0",
    "pre-git": "0.0.16",
    "q": "2.0.2",
    "time-grunt": "1.0.0"
  },
  "homepage": "https://github.com/bahmutov/qunit-promises",
  "keywords": [
    "test",
    "qunit",
    "plugin",
    "promises"
  ],
  "license": "MIT",
  "main": "qunit-promises.js",
  "pre-commit": "grunt && npm run gt",
  "pre-push": "grunt qunit",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/qunit-promises.git"
  },
  "scripts": {
    "test": "grunt && npm run gt",
    "gt": "node ./node_modules/gt qunit-promises.js test/node-tests.js --output",
    "coveralls": "node node_modules/lcov-filter/index.js cover/lcov.info test | ./node_modules/coveralls/bin/coveralls.js"
  }
}
